{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "lib": [
      "es7",
      "dom",
      "es2015.proxy"
    ],
    "outDir": "./build/esm5/",
    "alwaysStrict": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "esModuleInterop": true,
    "declaration": true,
    "preserveConstEnums": true
  },
  "include": [
    "src/**/*"
  ]
}
